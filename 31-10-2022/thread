package Thread;

public class fibona_series extends Thread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		fibona_series obj=new fibona_series();
		obj.start();
        fibona_series obj1=new fibona_series();
        obj1.start();
	}
	
	public void demo()
	{
		int n1=0, n2=1;
		int sum=0;
		System.out.println(+n1+" "+n2);
		for(int i=2;i<10;i++)
		sum=n1+n2;
		System.out.println(" "+sum);
		n1=n2;
		n2=sum;
	}

}
